#################################################################
#    Annotation properties
#################################################################

###  http://creativecommons.org/ns#license
<http://creativecommons.org/ns#license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/dataact#legalEffect
:legalEffect rdf:type owl:AnnotationProperty ;
             rdfs:comment "Legal effect description" ;
             rdfs:label "legal effect" .


###  http://www.semanticweb.org/dataact#legalSource
:legalSource rdf:type owl:AnnotationProperty ;
             rdfs:comment "Reference to legal document or provision" ;
             rdfs:label "legal source" .


###  http://www.semanticweb.org/dataact#qualifiedBy
:qualifiedBy rdf:type owl:AnnotationProperty ;
             rdfs:comment "Qualification or condition" ;
             rdfs:label "qualified by" .


###  http://www.semanticweb.org/dataact#ruleSource
:ruleSource rdf:type owl:AnnotationProperty ;
            rdfs:comment "Reference to specific article or recital" ;
            rdfs:label "rule source" .


###  http://www.semanticweb.org/dataact#temporalExtent
:temporalExtent rdf:type owl:AnnotationProperty ;
                rdfs:comment "Temporal period during which something holds" ;
                rdfs:label "temporal extent" .


###  http://www.w3.org/2004/02/skos/core#altLabel
<http://www.w3.org/2004/02/skos/core#altLabel> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#duration
xsd:duration rdf:type rdfs:Datatype .


